@charset "utf-8";
.childrenBody{
    overflow: hidden;
    position: absolute;
    top: 0;
    width: 100%;
    background: #fff;
}
/* 侧滑弹窗添加特殊类名 */
.gap-div,.approval-wrapper.gap-div{
    margin: 0 20px 0px;
    position: relative;
    float: left;
    width: calc(100% - 40px);
    overflow: hidden;
}
/* 高级搜索区域 */
.project-advanced-search{
    /* overflow: hidden; */
    display: inline-block;
    width: 100%;
}
.pas-title{
    border-bottom: 1px solid #D5DEEF;
    position: relative;
    padding: 9px 0;
    margin-bottom: 5px;
    width: 100%;
}
/* 子列表弹窗 */
/* .layui-layer-content {
    position: relative;
    margin: 0 20px;
    width: calc(100% - 40px);
    height: calc(100% - 55px);
} */
.subListBody{
    position: relative;
    overflow: hidden;
    margin: 0 20px;
}
.subListBody .pas-title,.pas-table-tool.only-rs-tool{
    padding-right: 75px;
}
.pas-table-tool{
    padding-right: 110px;
}
.pas-title .layui-inline{
    line-height: 32px;
}
.tabTable .pas-title .pas-title-left {
    margin-left: 155px;
}
.tabTable .advanced-search-card .layui-card-header{
	display: none;
}
.pas-title .pas-title-left .layui-btn>i.iconfont{
    font-size: 13px;
}
.pas-title .pas-title-left .layui-btn.upload-btn>i.iconfont{
    vertical-align: 2px;
}
.pas-title .pas-title-left .table-tool-btns .layui-btn>i.iconfont.icon-delete{
    font-size: 15px;
}
.pas-title-left .layui-btn~.layui-btn {
    margin-left: 7px;
}
.pas-title .line-after-span{
    margin-right: 15px;
    display: none;
}
/* 来源和状态 */
.source-state-btns{
	display: inline-block;
    border-left: 1px dashed #d5deef;
    margin-left: 10px;
    padding-left: 5px;
    padding-right: 15px;
    border-right: 1px dashed #d5deef;
}
/* 来源和状态最左侧没有其他标签时  start*/
.table-tool-btns div:nth-child(1).source-state-btns{
	border-left: none;
	padding-left: 0;
	margin-left: 0;
}
.table-tool-btns div:nth-child(1).source-state-btns .layui-checkbox-block{
  left: 0;
  transform: translateX(10px);
}
.table-tool-btns div:nth-child(1).source-state-btns .layui-checkbox-block::after {
    left: 15%;
}
.table-tool-btns div:nth-child(1).source-state-btns .layui-layer-tips{
    left: 0px;
}
.table-tool-btns div:nth-child(1).source-state-btns .layui-layer-tips i.layui-layer-TipsB {
	left: 10%;
}
/* 来源和状态最左侧没有其他标签时  end*/
.layui-checkbox-group>.layui-btn{
	margin-left: 8px;
	border-color: transparent !important;
	box-shadow: none;
}
.layui-checkbox-group{
    display: inline-block;
    position: relative;
}
/* tip提示框 */
.layui-checkbox-group .layui-layer-tips{
    z-index: 120;
    position: absolute;
    top: 25px;
    left: -100%;
    border-radius: 2px;
    white-space: nowrap;
}
.layui-checkbox-group .layui-layer-tips .layui-layer-content{
    background-color: rgba(0,0,0,0.8);
    padding: 0px 8px;
    color: #d5d5d6;
}
.layui-checkbox-group .layui-layer-tips i.layui-layer-TipsB {
    border-right-color: rgba(0,0,0,0.8);
    left: 42%;
    transform: rotate(90deg);
    top: -12px;
}
.close-tips-btn{
    font-size: 12px;
    cursor: pointer;
    margin-left: 5px;
}

.layui-checkbox-block{
    background: #fff;
    border-radius: 5px;
    box-shadow: 0px 3px 6px #D5DEEF;
    width: 160px;
    padding: 10px 10px 0;
    position: absolute;
    top: 40px;
    left: 57%;
    transform: translateX(-50%);
    z-index: 120;
    display: none;
}
.layui-checkbox-block ul{
    max-height: 260px;
    overflow: auto;
}
.layui-checkbox-block::after {
    content: "";
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    border: 6px solid #fff;
    border-left: 6px solid transparent;
    border-top: 6px solid transparent;
    border-bottom-right-radius: 3px;
    width: 0;
    transform: translateX(-50%) rotate(-135deg);
    top: -6px;
    box-shadow: 2px 2px 5px #f1f5fa;
}
.layui-checkbox-group li{
    padding: 6px 10px;
    /* border-bottom: 1px dashed #D5DEEF; */
}
.layui-checkbox-group li:nth-child(odd){
    background-color: #FFFFFF;
}
.layui-checkbox-group li:nth-child(even){
    background-color: #FAF9FE;
}
.layui-checkbox-group li .layui-form-checkbox[lay-skin=primary] {
    padding-left: 22px;
}
.layui-checkbox-group li .layui-form-checkbox[lay-skin=primary] i {
    width: 12px;
    height: 12px;
    line-height: 12px;
    margin-top: 3px;
}
.layui-checkbox-group li .layui-form-checkbox[lay-skin=primary] .layui-icon-ok:before {
    transform: scale(0.8);
    display: block;
    margin-left: -1px;
}
.checkbox-operate-btns{
    text-align: center;
    padding: 12px 0;
}
.checkbox-operate-btns .layui-btn{
    padding: 0 32px;
}

.table-tool-btns{
    display: inline-block; margin-right: 10px;
}
.project-advanced-search .layui-btn{
    height: 26px;
    line-height: 24px;
    font-size: 13px;
}
.project-advanced-search .layui-btn .icon-jiantou1{
	font-size: 12px !important;
    transform: scale(0.7);
    display: inline-block;
    font-weight: lighter;
    margin-left: 5px;
}
/* 展开/收起按钮 */
.unfold-btn,.unfold-btn.active,.unfold-btn.basic-btn:hover{
   	/* padding: 0 12px; */
   	transition: all 0s;
    background: transparent;
    color: #387cff;
    border: none;
    box-shadow: none;
    padding-right: 5px;
}
.unfold-btn .iconfont{
    font-size: 12px;
    margin-left: 6px;
    transform: rotate(180deg);
    display: inline-block;
    vertical-align: 1px;
    transition: all 0.3s;
}
.unfold-btn.active .iconfont{
	transform: rotate(0deg);
	vertical-align: 0;
}

.toolbar-other-btns{
    position: relative;
    margin-right: 7px;
}
.layui-btn+.table-tool-btns{
	 /*padding-left: 12px;
    margin-left: 12px; */
}
.toolbar-other-btns::before{
    content: "";
    position: absolute;
    left: -1px;
    top: 50%;
    transform: translateY(-50%);
    /* border-left: 1px dashed #D5DEEF; */
    height: 26px;
}
.toolbar-other-btns .layui-btn i.iconfont{
    margin-right: 4px;
}
.toolbar-other-btns .layui-btn i.iconfont.icon-daoru{
    font-size: 15px;
}
.toolbar-other-btns .layui-btn i.iconfont.icon-daochu{
    font-size: 12px;
}
.project-advanced-search .pas-title .search_btn{
    padding: 0 14px;
    margin-right: 8px;
}
/* 操作按钮组 */
.oper-right-btns{
    background: linear-gradient(-45deg,#276AFF,#559CFF);
    border-radius: 20px;
    box-shadow: 0px 5px 5px rgba(31, 101, 235, 0.2);
    display: inline-block;
    vertical-align: middle;
    position: relative;
 	margin: 0 7px 0 0px;
}
.layui-btn+.oper-right-btns{
	margin: 0 7px 0 17px;
}
.layui-btn+.oper-right-btns::before{
    content: "";
    border-right: 1px dashed #d5deef;
    height: 100%;
    display: inline-block;
    position: absolute;
    left: -10px;
    top: 50%;
    transform: translateY(-50%);
}
.oper-right-btns .layui-btn{
	margin-left: 0 !important;
    float: left;
    border-radius: 0;
    background: transparent;
    border-color: transparent;
    box-shadow: none;
    position: relative;
    padding: 0 14px;
}
.oper-right-btns .basic-btn:not(.layui-btn-primary):hover{
	background: transparent;
	border-color: transparent;
	box-shadow: none;
}
.oper-right-btns .layui-btn:not(:last-child):after{
    content: "";
    border-right: 1px solid #80b1ff;
    height: 14px;
    display: inline-block;
    position: absolute;
    right: 0px;
    top: 50%;
    transform: translateY(-50%);
}
/* 排序相关按钮   */
.sort-label{
    padding: 0 12px;
    border-left: 1px dashed #d5deef;
    line-height: 26px;
    display: inline-block;
    margin-left: 8px;
    color: #5C6989;
    display: none;
}
.project-advanced-search .pas-title .sort-btn,.layui-checkbox-group>.layui-btn{
	background: transparent;
    border-color: #c0d5fd;
    color: #3f85f6;
    transition: all 0s;
}
.layui-checkbox-group>.layui-btn{
	padding: 0 10px;
}
.project-advanced-search .pas-title .sort-btn:hover,
.project-advanced-search .pas-title .sort-btn.active,
.layui-checkbox-group>.layui-btn:hover,
.layui-checkbox-group>.layui-btn.active{
	background: linear-gradient(-45deg,#276AFF,#559CFF);
    box-shadow: 0px 3px 5px rgba(31, 101, 235, 0.2);
    color: #fff;
    border-color: #70b8ff;
}
.pas-title .pas-title-left .sort-btn>i.iconfont{
    font-size: 12px;
    margin-left: 7px;
}
/* 标题中的日期加减切换 */
.date-switch-group{
    margin: 0 23px 0 35px;
}
.date-switch-group button{
    width: 26px;
    height: 26px;
    line-height: 24px;
    cursor: pointer;
    text-align: center;
}
.date-switch-group input{
    text-align: center;
    width: 100px;
    border: none;
    vertical-align: middle;
}
.date-switch-group button i.iconfont{
    display: block;
    font-size: 12px;
    transform: rotate(90deg);
}

.date-switch-group button.date-next i.iconfont{
    transform: rotate(-90deg);
}

/* layui-tab里面嵌套时toolbar */
.popup-tab .layui-tab-content{
	 position: relative;
}
.layui-tab-content .pas-title{
    padding-top: 0px;
}
.layui-tab-content .project-table+.layui-table-view .layui-table-tool{
    top: 5px;
}
/* table工具栏只有toolbar无搜索和查询内容时 */
.only-toolbar{
	padding-top: 42px;
}
/* 开关 */
.switch-label{
    font-size: 13px;
    color: #5C6989;
}
.project-advanced-search .layui-form-switch{
   margin-top: 0px;
   margin-top: 0px;
   min-width: 48px;
   height: 26px;
   line-height: 26px;
   border-color: #EFF3F9;
   background-color: rgba(244,247,251,.7);
}
.project-advanced-search .layui-form-onswitch{
    background: transparent linear-gradient(to right,#56F20A,#11CB4F);
}
.project-advanced-search .layui-form-switch i {
    left: 3px;
    top: 2px;
    width: 20px;
    height: 20px;
    background-color: #fff;
    box-shadow: 0px 3px 5px rgba(31, 101, 235, 0.2);
}
.project-advanced-search .layui-form-onswitch i {
    left: 100%;
}
/* 输入框 */
.project-advanced-search .fuzzy-search,.template-contain .fuzzy-search{
    margin: 0 12px;
}
.template-contain .fuzzy-search{
    margin-top: 14px;
}
.project-advanced-search .fuzzy-search .fs-input,.template-contain .fuzzy-search .fs-input {
    text-indent: 0px;
    width: 120px;
    border-left: none;
}
/* 面板展开 */
.pas-content{
  /*   border: 2px solid #FAFCFD; */
    margin-bottom: 5px !important;
    width: 100%;
    background: #fafcfd;
}
/* 悬浮 */
.suspension-active{
    position: absolute;
    top: 78px;
    z-index: 102;
    box-shadow: 0px 3px 6px rgba(31, 101, 235, 0.2);
}

/* 显示统计内容 */
.state-statistics {
    /* padding: 0 10px 15px 20px; */
    padding: 10px;
    background: #fff;
    border-color: #FAFBFD;
}
.state-statistics .statistics-content {
    background: rgba(239, 243, 249, 0.2);
    border-radius: 5px;
    padding: 15px 22px;
}

.state-statistics .statistics-title{
    border: none;
    height: 45px;
    line-height: 45px;
    color: #5C6989;
}

.state-statistics .statistics-content>div{
    border-right: 1px solid rgba(213, 222, 239, 0.5);
}
.state-statistics .statistics-content>div:last-child{
    border-right: none;
}

/* 左侧状态统计 */
.states-left{
    width: calc(34% - 15px);
    margin-right: 15px;
}
.statistics-echarts{
    width: 53%;
    position: relative;
}
/* 图表 */
.echarts-parnets{
    width: 136px;
    height: 136px;
    border-radius: 100%;
    border: 1px solid #EBF2FD;
    position: relative;
}
.echarts-parnet,.echarts,.echarts-parnet::after{
    width: 88%;
    height: 88%;
    border-radius: 100%;
    position: absolute;
    top:50%;
    left: 50%;
    transform: translate(-50%,-50%);
}
.echarts-parnet{
    border: 1px solid #CBE0FE;
}
.echarts-parnet::after{
    content: "";
    box-shadow: 0px 0px 6px rgba(127, 148, 195, 0.3);
}
.echarts-title{
    position: absolute;
    top:50%;
    left: 50%;
    transform: translate(-50%,-50%);
    text-align: center;
}
.echarts-num{
    font-size: 22px;
    color: #5C6989;
    line-height: 40px;
}
.echarts-text{
    font-size: 13px;
    color: #7F94C3;
}
/* .echarts{
    box-shadow: 0px 0px 16px rgba(127, 148, 195, 0.3);
} */
/* 图标图例 */
.echarts-legend{
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    left: 52%;
}
.echarts-legend>p{
    line-height: 30px;
}
.echarts-legend>p .echarts-proportion{
   color: #FF6F00;
   margin-left: 5px;
}
.statistics-list{
    width: 47%;
    padding-left: 4px;
}
.statistics-list li{
    text-align: center;
    border: 1px solid #D5DEEF;
    color: #5C6989;
    border-radius: 3px;
    box-shadow: 0px 3px 6px rgba(127, 148, 195, 0.1);
    background: #fff;
    position: relative;
    overflow: hidden;
    padding: 10px;
    margin: 10px 0 0px 10px;
    width: calc(50% - 10px);
    float: left;
}
.statistics-list li:nth-child(-n+2){
    margin-top: 0px;
} 
.statistics-list li>h3{
    font-size: 20px;
    color: #FFA200;
    margin-bottom: 7px;
}
.check-indent{
    position: absolute;
    bottom: 0px;
    right: 0px;    
    width: 0;
    height: 0;
    border-top: 20px solid transparent;
    border-left: 20px solid transparent;
    border-right: 20px solid #3872ff;
}
.check-indent>i.iconfont{
    font-size: 12px;
    display: block;
    position: absolute;
    top: -12px;
    right: -20px;
    color: #fff;
    transform: scale(0.8);
}
/* 圆点标识 */
.circle-indent{
    width: 12px;
    height: 12px;
    border-radius: 100%;
    border: 1px solid transparent;
    display: inline-block;
}
.circle-indent::after{
    display: inline-block;
    content: "";
    width: 6px;
    height: 6px;
    border-radius: 100%;
    margin: 2px;
    float: left;
}
/* 草稿 */
.draft-indent{
    border-color: #C6CFE5;
    color: #7F94C3;
}
.draft-indent::after{
    background: #7F94C3;
}
/* 审批中 */
.approval-indent{
    border-color: red;
    color: red;
}
.approval-indent::after{
    background: #FB0167;
}
/* 进行中 */
.ongoing-indent{
    border-color: #FED999;
    color: #FFB838;
}
.ongoing-indent::after{
    background: #FFB838;
}
/* 封账 */
.closing-indent{
    border-color: #9CF9E4;
    color: #11CB8C;
}
.closing-indent::after{
    background: #0AF2BB;
}

.closing-accounts{
    border-color: #FB7E22;
    color: #FB7E22;
}
.closing-accounts .tag-circle {
    border-color: #FB7E22;
    background: #FB7E22;
}

/* 右侧比较统计 */
.states-right{
    width: 66%;
}
.compare-item{
    padding: 10px 18px;
}
.compare-item:first-child{
    padding-left: 0px;
}
.compare-item:last-child{
    padding-right: 0px;
}
.compare-item-title>span{
    color: #FF6F00;
    padding-bottom: 10px;
    margin-right: 10px;
    position: relative;
}
.compare-item-title>span::after{
    content: "";
    border-bottom: 1px solid #FFA200;
    width: 25px;
    display: inline-block;
    position: absolute;
    bottom: 0;
    left: 0;
}
/* 进度条 */
.layui-progress,.layui-progress-bar{
    height: 10px;
    background: #EFF3F9;
}
.layui-progress-bar{
    background: linear-gradient(to right,rgba(255,162,0,0.6),rgba(255,210,32,0.6));
}
.layui-progress:last-child .layui-progress-bar{
    background: linear-gradient(to right,rgba(127,148,195,0.6),rgba(213,222,239,0.6));
}
.compare-item-title{
    margin-bottom: 16px;
    color: #5C6989;
}
.compare-item>p{
    padding: 10px 0;
    font-size: 13px;
    color: #7F94C3;
}
.compare-item>p span span{
    color: #5C6989;
    margin: 0 5px;
}
.progress-number{
    color: #FF6F00;
}

/* 高级搜索 */
.advanced-search-card .layui-card-header{
    border-bottom: none;
    padding: 8px 10px 0;
    height: 32px;
    line-height: 32px;
}
.advanced-search-card .layui-card-body>form{
    background: #FAFCFD;
}
.lch-title{
    padding-left: 8px;
    color: #1F65EB;
    border-left: 2px solid #1F65EB;
    font-size: 14px;
    margin-top: 5px;
    line-height: 14px;
}
.advanced-search-card{
    padding: 0px 0px 8px; 
    box-shadow: none;
}
.advanced-search-card .layui-card-body {
    background: #FAFCFD;
    padding: 5px 0 0;
    margin: 0 10px 0px;
}
.advanced-search-card .layui-card-body .layui-inline[class*="layui-col"]{
    min-height: 40px;
}
/* 对于内容太多的一行放三个 */
.advanced-search-card .layui-card-body .layui-inline.layui-col-md4>label{
    width: 180px;
}
.advanced-search-card .layui-card-body .layui-inline.layui-col-md4>.layui-input-inline{
    width: calc(100% - 190px);
}
.advanced-search-card .layui-card-body .layui-inline>label,
.advanced-search-card .layui-card-body .drop-down-date.layui-col-md4>label{
    line-height: 32px;
    text-align: right;
    display: inline-block;
    width: 140px;
    font-size: 13px;
}
.layui-form-checkbox span{
	font-size: 13px;
}
.advanced-search-card .layui-card-body .layui-inline>.layui-input-inline,
.advanced-search-card .layui-card-body .drop-down-date.layui-col-md4>.layui-input-inline {
    width: calc(100% - 150px);
    position: relative;
}

.advanced-search-card .layui-form-select .layui-edge {
    right: 12px;
}

/* 日期下拉组合 */
.drop-down-date .layui-form-select{
    width: 55px;
    float: left;
}
.drop-down-date .layui-form-select .layui-input {
    border-radius: 3px 0 0 3px;
}
.drop-down-date .date-input{
    width: calc(100% - 55px);
    float: left;
    border-radius: 0 3px 3px 0;
    border-left: none;
}

/* 资金计划统计区域 */
.state-statistics-contain{
   background: #FCFDFE;
   padding: 15px 0;
}
.state-statistics .layui-col-md4{
    border-right: 1px solid #E8EDF6;
    padding-left: 80px;
}
.state-statistics .layui-col-md4:nth-last-of-type(1){
    border-right: none;
}
.statistics-text{
    color: #5C6989;
    padding-bottom: 8px;
    position: relative;
    margin-bottom: 12px;
}
.statistics-text>span{
    color: #FF6F00;
    margin-right: 10px;
}
.statistics-text>span::after{
    content: "";
    border-bottom: 1px solid #FFA200;
    width: 25px;
    display: inline-block;
    position: absolute;
    left: 0;
    bottom: 0;
}
.approval-amount {
    color: #5C6989;
    float: left;
    width: 50%;
}
.approval-amount h4{
   color: #7F94C3;
}
.amount-list{
    margin: 10px 0;
}
.amount-list p{
    line-height: 24px;
    font-size: 13px;
}
/* 正常紧凑切换 */
.switch-layout{
	display: inline-block;
}
.switch-layout .layui-form-onswitch em {
    top: -1px;
}
.project-advanced-search .switch-layout .layui-form-onswitch {
    background: transparent linear-gradient(-45deg,#276AFF,#559CFF);
    min-width: 62px;
}
.project-advanced-search .switch-layout .layui-form-switch {
    min-width: 62px;
    background-color: rgb(242, 245, 250);
}
/* 紧凑选中下的表格样式 */
.compact-table+.layui-table-view .layui-table td ,.compact-table+.layui-table-view .layui-table th{
    font-size: 12px;
