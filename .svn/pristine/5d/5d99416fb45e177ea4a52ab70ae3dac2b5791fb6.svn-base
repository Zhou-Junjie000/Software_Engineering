    .capital-table+.layui-table-view .layui-table-cell {
        padding: 0;
    }
    .capital-status {
        width: 65px;
        height: 20px;
        line-height: 20px;
    }
    .calculate-mode>span {
        margin-left: 0px;
    }
    .tip-entry {
        width: 16px;
        height: 16px;
    }
    .tip-entry i.iconfont {
        transform: scale(0.90);
        margin: 1px;
    }
    .layui-form-radio * {
	    font-size: 12px;
	    vertical-align: -3px;
	}
}
@media screen and (max-width: 1360px){
	.advanced-search-card .layui-card-body .layui-inline>label, .advanced-search-card .layui-card-body .drop-down-date.layui-col-md4>label {
	    line-height: 28px;
	}
	.layui-form-checkbox span{
		font-size: 12px;
	}
}