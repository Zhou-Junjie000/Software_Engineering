    position: relative;
    z-index: 1;
    background: rgba(0,0,0,0.8);
    line-height: 16px;
    font-size: 12px;
    vertical-align: 4px;
    width: 30px;
    white-space: normal;
    display: block;
    padding: 1px 3px;
}

.tip-title::after {
    content: "";
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    border: 5px solid rgba(0,0,0,0.8);
    border-left: 5px solid transparent;
    border-top: 5px solid transparent;
    width: 0;
    transform: translateX(-50%) rotate(135deg);
    bottom: -5px;
}

