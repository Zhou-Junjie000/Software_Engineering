.sh-card-list{
    padding: 10px 5px;
}
.sh-card-list li{
    border-bottom: 1px dotted #D5DEEF;
    position: relative;
    padding: 20px 10px;
}
.sh-card-list li:hover{
    background: #FFFEFC;
}
.sh-card-list li>.sh-list-left{
    float: left;
}
.sh-card-list li>.sh-list-left>span{
    background: #D5DEEF;
    width: 18px;
    height: 18px;
    border-radius: 5px;
    transform: rotate(45deg);
    display: block;
    position: absolute;
    top: 19px;
    left: 5px;
    z-index: 0;
}
.sh-card-list li>.sh-list-left>i{
    position: absolute;
    color: #5C6989;
    height: 18px;
    line-height: 18px;
    text-align: center;
    z-index: 1;
}
.sh-card-list li:nth-child(1)>.sh-list-left>span{
    background: #FB013D;
}
.sh-card-list li:nth-child(2)>.sh-list-left>span{
    background: #FF6F00;
}
.sh-card-list li:nth-child(3)>.sh-list-left>span{
    background: #FFA200;
}
.sh-card-list li:nth-child(1)>.sh-list-left>i,
.sh-card-list li:nth-child(2)>.sh-list-left>i,
.sh-card-list li:nth-child(3)>.sh-list-left>i{
    color: #fff;
}
.sh-card-list li>h3{
    color: #1F65EB;
    margin-left: 25px;
    cursor: pointer;
}
.sh-card-list li:hover>h3{
    color: #FFA200;
}
.sh-card-list li>.sh-list-right{
    float: right;
    color: #5C6989;
}
.sh-card-list li>.sh-list-right>i{
    visibility: hidden;
    cursor: pointer;
}
.sh-card-list li>.sh-list-right>i:hover{
    color: #FB013D;
}
.sh-card-list li:hover>.sh-list-right>i{
    visibility: visible;
}

.sh-card-remove{
    background: #F9FAFD;
    color: #5C6989;
    text-align: center;
    width: 320px;
    height: 40px;
    line-height: 40px;
    border-radius: 100px;
    margin: 0 auto;
    display: block;
}
.sh-card-remove:hover{
    background: #EFF3F9;
    color: #1F65EB;
}