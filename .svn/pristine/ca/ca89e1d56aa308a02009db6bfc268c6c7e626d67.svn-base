/* 当日日期分割区域 */
.transparentBg{
    width: 60%;
    background: rgba(213, 222, 239, 0.3);
    position: absolute;
    top: 40px;
	left: 0;
	bottom: 0;
}

/*开始刻度线*/
.orangeLine{
    height: 100%;
    border-left: 1px solid #FB0167;
	position: absolute;
	top: 0px;
}
.orangeLine>span{
    color: #fff;
    background: #FB0167;
    border-radius: 20px;
    display: block;
    margin-left: -13px;
	font-size: 12px;
	padding: 1px 6px;
	line-height: 1;
	position: relative;
}
.orangeLine>span:after{
    content: "";
    border: 3px solid #FB0167;
    border-left: 3px solid transparent;
    border-top: 3px solid transparent;
    border-bottom-right-radius: 1px;
    position: absolute;
    bottom: -3px;
    left: 50%;
    transform: translateX(-50%) rotate(45deg);
}

.startDay{
	left: 210px;
}

/* 结束刻度线 */
.endDay{
	left: 1395px;
	border-left-style: dashed;
}